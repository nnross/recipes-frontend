{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/function-component-definition": [
          0
        ],
        "no-underscore-dangle": [
          0
        ],
        "import/ignore": [
          0
        ],
        "import/no-extraneous-dependencies": [
          0
        ]
      }
}
